{
    "name": "pounard/wps",
    "description": "WebPhotoSync - Own photo synchronisation web service",
    "authors": [
        {
            "name": "Pierre Rineau",
            "email": "pierre.rineau-php@processus.org",
            "homepage": "http://blog.processus.org",
            "role": "Developer"
        }
    ],
    "autoload": {
        "classmap": [ "lib/" ],
        "psr-0": {
            "Account": "modules/account/src/",
            "Contact": "modules/contact/src/",
            "Smvc": "modules/core/src/",
            "Wps": "src/"
        }
    },
    "require": {
        "php-config/php-config": "dev-master",
        "ircmaxell/password-compat": "dev-master",
        "pimple/pimple": "2.*@dev",
        "zendframework/zend-validator": "2.*",
        "zendframework/zend-filter": "2.*",
        "doctrine/cache": "1.3.*"
    },
    "replace": {
        "pounard/smvc-account": "self.version",
        "pounard/smvc-contact": "self.version",
        "pounard/smvc-core": "self.version"
    }
}
